{
  "name": "@hemicycle/react",
  "version": "0.1.4",
  "description": "Core Hemicycle layout computations",
  "keywords": [
    "hemicycle",
    "parliament",
    "seating-chart",
    "seat-layout",
    "visualization",
    "svg",
    "component",
    "react",
    "typescript",
    "geometry"
  ],
  "homepage": "https://github.com/GabrielVidal1/hemicycle#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/GabrielVidal1/hemicycle.git"
  },
  "bugs": {
    "url": "https://github.com/GabrielVidal1/hemicycle/issues"
  },
  "license": "MIT",
  "author": "Gabriel Vidal <gvidalayrinhac@gmail.com>",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.cts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "NODE_ENV=development tsdown --watch",
    "build": "tsdown --config-loader unrun",
    "prepublishOnly": "yarn build",
    "lint": "eslint \"src/**/*.ts\""
  },
  "dependencies": {
    "@hemicycle/core": "0.1.4",
    "@hemicycle/vanilla": "0.1.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}